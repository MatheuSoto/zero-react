{
  "name": "start-project",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"npm run postcss\" \"npm run dev\"",
    "dev": "SET NODE_ENV=development & webpack-dev-server",
    "postcss": "postcss src/assets/css/styles.css -o src/assets/css/build.css -w",
    "postcss:build": "SET NODE_ENV=production & postcss src/assets/css/styles.css -o src/assets/css/build.css",
    "build": "npm run postcss:build & webpack --mode production",
    "start:dev": "npx serve build -s",
    "serve": "npm run build && npm run start:dev"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@fullhuman/postcss-purgecss": "^2.3.0",
    "babel-loader": "^8.1.0",
    "concurrently": "^5.2.0",
    "copy-webpack-plugin": "^6.0.3",
    "css-loader": "^3.6.0",
    "cssnano": "^4.1.10",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "mini-css-extract-plugin": "^0.9.0",
    "postcss": "^7.0.32",
    "postcss-cli": "^7.1.1",
    "script-ext-html-webpack-plugin": "^2.1.4",
    "stylelint-config-recommended": "^3.0.0",
    "tailwindcss": "^1.4.6",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-pwa-manifest": "^4.2.0",
    "workbox-webpack-plugin": "^5.1.3"
  },
  "dependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0"
  }
}
